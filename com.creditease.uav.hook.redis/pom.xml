			<scope>provided</scope>
		</dependency>
		
		<dependency>
			<groupId>com.creditease.moniter</groupId>
			<artifactId>com.creditease.uav.cache.redis</artifactId>
			<scope>provided</scope>
		</dependency>
		
		<dependency> 
			<groupId>aredis.sourceforge.net</groupId>
			<artifactId>aredis-api</artifactId>
			<version>1.4</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/aredis-api-1.4.jar</systemPath>
		</dependency>
		
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>1.2</version>
		</dependency>	
	</dependencies>
	
	<build>
        <plugins>
            <plugin>
                <artifactId>maven-jar-plugin</artifactId>
                <executions>
                    <execution>
                        <id>jedis</id>
                        <goals><goal>jar</goal></goals>
                        <phase>package</phase>
                        <configuration>
                            <classifier>jedis</classifier>
                            <includes>
                                <include>**/com/creditease/uav/hook/redis/jedis/**</include>
                            </includes>
                        </configuration>
                    </execution>
                    <execution>
                        <id>lettuce</id>
                        <goals><goal>jar</goal></goals>
                        <phase>package</phase>
                        <configuration>
                            <classifier>lettuce</classifier>
                            <includes>
                                <include>**/com/creditease/uav/hook/redis/lettuce/**</include>
                            </includes>
                        </configuration>
                    </execution>
                    <execution>
                         <id>lettuce5x</id>
                         <goals><goal>jar</goal></goals>
                         <phase>package</phase>
                         <configuration>
                             <classifier>lettuce5x</classifier>
                             <includes>
                                 <include>**/com/creditease/uav/hook/redis/lettuce5x/**</include>
                             </includes>
                         </configuration>
                     </execution>
                    <execution>
                        <id>aredis</id>
                        <goals><goal>jar</goal></goals>
                        <phase>package</phase>
                        <configuration>
                            <classifier>aredis</classifier>
                            <includes>
                                <include>**/com/creditease/uav/hook/redis/aredis/**</include>
                            </includes>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        
    </build>
</project>